{
  "name": "jspgrequestframework",
  "version": "1.0.8",
  "description": "Request framework for POSTGRESQL.",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "typings": "./dist/src/index.d.ts",
  "scripts": {
    "setup": "npm install",
    "build": "tsc",
    "test": "tsc && jest --forceExit"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "author": "Hai Pham",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/pg": "^7.1.5",
    "jsrequestframework": "^1.1.3",
    "pg": "^7.3.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.5",
    "@types/uuid": "^3.4.3",
    "jest": "^21.2.1",
    "ts-jest": "^21.1.4",
    "uuid": "^3.1.0"
  }
}
